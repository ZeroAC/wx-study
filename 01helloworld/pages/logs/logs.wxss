.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0rpx;
    padding: 0rpx;
    width: 750rpx;
}
.headLine{
    font-size:50rpx;
    font-style:bold;
    color:#f0f;
    margin: 100rpx 0rpx;
}
.movie-item{
    display: flex;
    justify-content: space-between;
    width: 750rpx;
    margin-top: 50rpx;
}
.movie-text{
    font-size: 40rpx;
    color: darkred;
    font-weight: bold;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}